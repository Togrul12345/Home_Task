
@model ServiceDto;
<form method="post" enctype="multipart/form-data">
    <div class="mb-3 row">
        <label for="staticEmail" asp-for="Title" class="col-sm-2 col-form-label">Title</label>
        <div class="col-sm-10">
            <input  asp-for="Title" class="form-control" id="staticEmail" >
        </div>
        <span class="text-danger" asp-validation-for="Title"></span>
    </div>
  
    <div class="mb-3 row">
        <label for="inputPassword" asp-for="Description" class="col-sm-2 col-form-label">Description</label>
        <div class="col-sm-10">
            <input asp-for="Description" class="form-control" id="inputPassword">
        </div>
        <span class="text-danger" asp-validation-for="Description"></span>
    </div>
    <div class="mb-3 row">
 
        <input class="form-control"asp-for="Img" id="formFile">
        <span class="text-danger" asp-validation-for="Img"></span>
    </div>
 
    
    <button type="submit">Submit</button>
</form>