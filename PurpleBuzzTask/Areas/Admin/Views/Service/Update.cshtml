@using PurpleBuzzTask.Models;
@model Service;
<form method="post">
    <div class="mb-3 row">
        <label for="staticEmail" asp-for="Title" class="col-sm-2 col-form-label">Title</label>
        <div class="col-sm-10">
            <input type="text" asp-for="Title" class="form-control-plaintext" id="staticEmail" value="email@example.com">
        </div>
    </div>
    <div class="mb-3 row">
        <label for="inputPassword" asp-for="Description" class="col-sm-2 col-form-label">Description</label>
        <div class="col-sm-10">
            <input asp-for="Description" class="form-control" id="inputPassword">
        </div>
    </div>
    <div class="mb-3 row">
        <label asp-for="MainImageUrl" class="col-sm-2 col-form-label">img url</label>
        <div class="col-sm-10">
            <input asp-for="MainImageUrl" class="form-control" id="inputPassword">
        </div>
    </div>
</form>
